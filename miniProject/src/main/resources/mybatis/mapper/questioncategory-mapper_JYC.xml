<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.green.miniProject.dao.IQuestionCategoryDAO_JYC">
	<select id="getQcno">
		SELECT qcno FROM questioncategory WHERE qcname = #{category};
  	</select>
  	<select id="getQcname">
  		SELECT qcname FROM questioncategory WHERE qcno = #{qcno};
  	</select>
  	<select id="getAllQuestionCategoryList" resultType="com.green.miniProject.domain.QuestionCategory">
  		SELECT * FROM questioncategory;
  	</select>
  	
  	
  	
  	
</mapper>  